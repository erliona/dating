{
  "title": "Dating App - Debug Dashboard",
  "tags": [
    "dating",
    "debug",
    "logs",
    "troubleshooting"
  ],
  "timezone": "browser",
  "editable": true,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Live Application Logs (All Levels)",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{container_name=~\".*bot.*\"} | json | line_format \"{{.timestamp}} [{{.level}}] {{.logger}}: {{.message}}\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 12
      }
    },
    {
      "id": 2,
      "title": "Error Logs Only",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{container_name=~\".*bot.*\"} | json | level =~ \"ERROR|CRITICAL\" | line_format \"{{.timestamp}} [{{.level}}] {{.logger}}:{{.function}}:{{.line}} - {{.message}}\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      },
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 12,
        "h": 10
      }
    },
    {
      "id": 3,
      "title": "Warning Logs",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{container_name=~\".*bot.*\"} | json | level = \"WARNING\" | line_format \"{{.timestamp}} [{{.level}}] {{.logger}}:{{.function}} - {{.message}}\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      },
      "gridPos": {
        "x": 12,
        "y": 12,
        "w": 12,
        "h": 10
      }
    },
    {
      "id": 4,
      "title": "Log Levels Distribution",
      "type": "timeseries",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "sum by (level) (count_over_time({container_name=~\".*bot.*\"} | json [1m]))",
          "refId": "A",
          "legendFormat": "{{level}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "gridPos": {
        "x": 0,
        "y": 22,
        "w": 12,
        "h": 8
      }
    },
    {
      "id": 5,
      "title": "Events Timeline (by event_type)",
      "type": "timeseries",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "sum by (event_type) (count_over_time({container_name=~\".*bot.*\"} | json | event_type != \"\" [1m]))",
          "refId": "A",
          "legendFormat": "{{event_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "gridPos": {
        "x": 12,
        "y": 22,
        "w": 12,
        "h": 8
      }
    },
    {
      "id": 6,
      "title": "Bot Service Health",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "up{job=\"cadvisor\",name=~\".*bot.*\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "options": {
                "0": {
                  "text": "Down",
                  "color": "red"
                },
                "1": {
                  "text": "Running",
                  "color": "green"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          }
        }
      },
      "gridPos": {
        "x": 0,
        "y": 30,
        "w": 6,
        "h": 4
      }
    },
    {
      "id": 7,
      "title": "Bot CPU Usage %",
      "type": "gauge",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{name=~\".*bot.*\"}[5m]) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 50,
                "color": "yellow"
              },
              {
                "value": 80,
                "color": "red"
              }
            ]
          }
        }
      },
      "gridPos": {
        "x": 6,
        "y": 30,
        "w": 6,
        "h": 4
      }
    },
    {
      "id": 8,
      "title": "Bot Memory Usage",
      "type": "gauge",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "container_memory_usage_bytes{name=~\".*bot.*\"} / container_spec_memory_limit_bytes{name=~\".*bot.*\"} * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 70,
                "color": "yellow"
              },
              {
                "value": 90,
                "color": "red"
              }
            ]
          }
        }
      },
      "gridPos": {
        "x": 12,
        "y": 30,
        "w": 6,
        "h": 4
      }
    },
    {
      "id": 9,
      "title": "Database Connections",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "pg_stat_activity_count",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 50,
                "color": "yellow"
              },
              {
                "value": 80,
                "color": "red"
              }
            ]
          }
        }
      },
      "gridPos": {
        "x": 18,
        "y": 30,
        "w": 6,
        "h": 4
      }
    },
    {
      "id": 10,
      "title": "API Requests by Endpoint",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{container_name=~\".*bot.*\"} | json | event_type =~ \".*request.*|.*response.*\" | line_format \"{{.timestamp}} {{.method}} {{.path}} - Status: {{.status}} - {{.message}}\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": false,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      },
      "gridPos": {
        "x": 0,
        "y": 34,
        "w": 12,
        "h": 8
      }
    },
    {
      "id": 11,
      "title": "Database Queries",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{container_name=~\".*bot.*\"} | json | logger =~ \".*repository.*|.*db.*\" | line_format \"{{.timestamp}} {{.logger}}:{{.function}} - {{.message}}\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      },
      "gridPos": {
        "x": 12,
        "y": 34,
        "w": 12,
        "h": 8
      }
    },
    {
      "id": 12,
      "title": "User Actions (Profile, Match, Like)",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{container_name=~\".*bot.*\"} | json | event_type =~ \"profile_.*|match_.*|like_.*|photo_.*\" | line_format \"{{.timestamp}} [{{.event_type}}] User:{{.user_id}} - {{.message}}\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      },
      "gridPos": {
        "x": 0,
        "y": 42,
        "w": 24,
        "h": 10
      }
    }
  ]
}
