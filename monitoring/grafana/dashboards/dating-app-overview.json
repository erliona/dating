{
  "title": "Dating App - System Overview",
  "tags": [
    "dating",
    "overview",
    "infrastructure"
  ],
  "timezone": "browser",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Services Status",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "up",
          "refId": "A",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "options": {
                "0": {
                  "text": "Down",
                  "color": "red"
                },
                "1": {
                  "text": "Up",
                  "color": "green"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          }
        }
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 8,
        "h": 4
      }
    },
    {
      "id": 2,
      "title": "Container CPU Usage",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{name=~\".+\"}[5m]) * 100",
          "legendFormat": "{{name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "gridPos": {
        "x": 8,
        "y": 0,
        "w": 8,
        "h": 8
      }
    },
    {
      "id": 3,
      "title": "Container Memory Usage",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "container_memory_usage_bytes{name=~\".+\"} / 1024 / 1024",
          "legendFormat": "{{name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "gridPos": {
        "x": 16,
        "y": 0,
        "w": 8,
        "h": 8
      }
    },
    {
      "id": 4,
      "title": "PostgreSQL Active Connections",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "pg_stat_database_numbackends{datname!=\"\"}",
          "legendFormat": "{{datname}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      }
    },
    {
      "id": 5,
      "title": "Container Network Traffic",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(container_network_receive_bytes_total{name=~\".+\"}[5m])",
          "legendFormat": "{{name}} - RX",
          "refId": "A"
        },
        {
          "expr": "rate(container_network_transmit_bytes_total{name=~\".+\"}[5m])",
          "legendFormat": "{{name}} - TX",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      }
    },
    {
      "id": 6,
      "title": "All Container Logs",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{job=\"docker\"}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 12,
        "h": 10
      },
      "options": {
        "showLabels": true,
        "showTime": true,
        "wrapLogMessage": true,
        "sortOrder": "Descending",
        "enableLogDetails": true,
        "dedupStrategy": "none"
      },
      "description": "All logs from Docker containers. If no logs appear, ensure monitoring stack is running with: docker compose --profile monitoring up -d"
    },
    {
      "id": 7,
      "title": "Bot Application Logs (JSON Parsed)",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{job=\"docker\", container_name=~\".*bot.*\"} | json | line_format \"[{{.level}}] {{.message}} | module={{.module}} func={{.function}}\"",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 12,
        "h": 10
      },
      "options": {
        "showLabels": true,
        "showTime": true,
        "wrapLogMessage": true,
        "sortOrder": "Descending",
        "enableLogDetails": true,
        "dedupStrategy": "none"
      },
      "description": "Bot application logs with JSON parsing to extract structured fields like level, message, module, and function"
    },
    {
      "id": 8,
      "title": "Bot Error and Warning Logs",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{job=\"docker\", container_name=~\".*bot.*\"} | json | level=~\"ERROR|WARNING\" | line_format \"[{{.level}}] {{.message}}\"",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 26,
        "w": 12,
        "h": 8
      },
      "options": {
        "showLabels": true,
        "showTime": true,
        "wrapLogMessage": true,
        "sortOrder": "Descending",
        "enableLogDetails": true,
        "dedupStrategy": "none"
      },
      "description": "Only ERROR and WARNING level logs from the bot application"
    },
    {
      "id": 9,
      "title": "Bot Events Timeline",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{job=\"docker\", container_name=~\".*bot.*\"} | json | event_type!=\"\" | line_format \"Event: {{.event_type}} - {{.message}}\"",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 26,
        "w": 12,
        "h": 8
      },
      "options": {
        "showLabels": true,
        "showTime": true,
        "wrapLogMessage": true,
        "sortOrder": "Descending",
        "enableLogDetails": true,
        "dedupStrategy": "none"
      },
      "description": "Timeline of bot lifecycle events (startup, shutdown, config_loaded, etc.)"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 27,
  "version": 1,
  "time": {
    "from": "now-1h",
    "to": "now"
  }
}