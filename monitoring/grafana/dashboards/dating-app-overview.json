{
  "dashboard": {
    "title": "Dating App - Overview",
    "tags": ["dating", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Container Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=~\"cadvisor|postgres|traefik\"}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 8,
          "h": 4
        }
      },
      {
        "id": 2,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total[5m]) * 100",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "x": 8,
          "y": 0,
          "w": 8,
          "h": 8
        }
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "container_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "x": 16,
          "y": 0,
          "w": 8,
          "h": 8
        }
      },
      {
        "id": 4,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_database_numbackends",
            "legendFormat": "{{datname}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 5,
        "title": "Network Traffic",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_network_receive_bytes_total[5m])",
            "legendFormat": "{{name}} - RX",
            "refId": "A"
          },
          {
            "expr": "rate(container_network_transmit_bytes_total[5m])",
            "legendFormat": "{{name}} - TX",
            "refId": "B"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 6,
        "title": "Recent Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"docker\"}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 24,
          "h": 8
        },
        "options": {
          "showLabels": true,
          "showTime": true,
          "wrapLogMessage": true
        }
      }
    ],
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}
